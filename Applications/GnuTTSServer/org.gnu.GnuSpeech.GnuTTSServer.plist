<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>org.gnu.GnuSpeech.GnuTTSServer</string>
	<key>ProgramArguments</key>
	<array>
		<string>/Library/GnuSpeech/GnuTTSServer</string>
	</array>
	<key>StandardErrorPath</key>
	<string>/Library/Logs/GnuSpeechDaemon.log</string>
	<key>StandardOutPath</key>
	<string>/Library/Logs/GnuSpeechDaemon.log</string>
	<key>OnDemand</key>
	<true/>
	<key>KeepAlive</key>
	<false/>
</dict>
</plist>
